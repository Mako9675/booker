<h1>Books</h1>
<% @books.each do |book| %>
<!-- title -->
<p>Title Body</p>
<span>
  <%= link_to book.title, book_path(book.id) %>
</span>

<%= link_to "Edit",edit_book_path(book.id) %>


<% end %>

<!-- 投稿box -->
<h2>New book</h2>
<%= form_with model: @books,url: '/lists',method: :post do |f| %>

 <h4>title</h4>
 <%= f.text_field :title %>

 <h4>body</h4>
 <%= f.text_area :body %>

 <%= f.submit 'Create Book' %>
<% end %>
