<h1>Books</h1>
<% @books.each do |book| %>
<!-- title -->
<p>Title</p><p>Body</p>
<span><%= book.title %></span>

<% end %>

<!-- 投稿box -->
<h2>New book</h2>
<%= form_with model: @books,url: '/lists',method: :post do |f| %>
 <h4>title</h4>
 <%= f.text_field :title %>

 <h4>body</h4>
 <%= f.text_area :body %>

 <%= f.submit 'Create Book' %>
<% end %>
